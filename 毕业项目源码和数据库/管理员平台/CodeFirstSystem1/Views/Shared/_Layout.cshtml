
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0, minimal-ui">
    <title>美食网管理程序</title>
    <link href="~/Zoter/jquery-jvectormap-2.0.2.css" rel="stylesheet">
    <link href="~/Zoter/bootstrap.min.css" rel="stylesheet" type="text/css">
    <link href="~/Zoter/style.css" rel="stylesheet" type="text/css">
</head>
<body class="fixed-left" style="overflow: visible;">

    <div id="wrapper">
        <div class="left side-menu">
            <button type="button" class="button-menu-mobile button-menu-mobile-topbar open-left waves-effect">
                <i class="ion-close"></i>
            </button>
            <div class="topbar-left">
                <div class="text-center">
                    <a href="#" class="logo"><font color="white" size="20">山珍海味</font></a>
                </div>
            </div>
            <div class="sidebar-inner niceScrollleft" tabindex="5000" style="overflow: hidden; outline: none;">
                <div id="sidebar-menu" class="active">
                    <ul>
                        <li class="has_sub">
                            <a href="javascript:void(0);" class="waves-effect"><span>类别管理</span></a>
                            <ul class="list-unstyled">
                                <li><a href="/Type/Index">大类级</a></li>
                                <li><a href="/Typemin/Index">小类级</a></li>
                            </ul>
                        </li>
                        <li class="has_sub">
                            <a href="javascript:void(0);" class="waves-effect"><span>其他管理</span></a>
                            <ul class="list-unstyled">
                                <li><a href="/Sfav/Index">时蔬管理</a></li>
                                <li><a href="/DietNous/Index">健康知识管理</a></li>
                            </ul>
                        </li>
                    </ul>
                </div><div class="clearfix"></div>
            </div>

        </div><div class="content-page">
            <div class="content">
                <div class="topbar">
                    <nav class="navbar-custom">
                        <ul class="list-inline float-right mb-0">
                            <li class="list-inline-item dropdown notification-list text-white"><span>欢迎你，@User.Identity.Name</span></li>
                            <li class="list-inline-item dropdown notification-list">
                                <a class="nav-link dropdown-toggle arrow-none waves-effect nav-user" data-toggle="dropdown" href="#" role="button" aria-haspopup="false" aria-expanded="false">
                                    <img src="~/Zoter/avatar-1.jpg" alt="user" class="rounded-circle">
                                </a>
                                <div class="dropdown-menu dropdown-menu-right profile-dropdown ">
                                    <a class="dropdown-item" href="/Login/TLogout">注销账号</a>
                                </div>
                            </li>
                        </ul>

                        <div class="clearfix">
                        </div>
                    </nav>
                </div><div class="page-content-wrapper ">
                    <div class="container-fluid">
                        @RenderBody()
                    </div>
                </div>
            </div><footer class="footer">© @DateTime.Now  by 山珍海味美食网</footer>
        </div>
    </div>
    <script src="~/Zoter/jquery.min.js"></script>
    <script src="~/Zoter/popper.min.js"></script>
    <script src="~/Zoter/bootstrap.min.js"></script>
    <script src="~/Zoter/jquery.nicescroll.js"></script>
    <script src="~/Zoter/app.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>