        @*标签页*@
        <div class="tab_content">
            <div class="allfood">
                <ul id="myTab" class="nav nav-tabs">
                    @foreach (var item in types)
                    {
                        <li class="dropdown">
                            @if (item.PID == null)
                            {
                                <a href="#" id="myTabDrop1" class="dropdown-toggle" data-toggle="dropdown">
                                    @item.TypeName
                                    <b class="caret"></b>
                                </a>
                                <ul class="dropdown-menu" role="menu" aria-labelledby="myTabDrop1">
                                    @foreach (var item1 in types)
                                    {
                                        if (item.TypeID == item1.PID)
                                        {
                                            <li><a href="#@item1.TEName" tabindex="-1" data-toggle="tab">@item1.TypeName</a></li>
                                        }
                                    }
                                </ul>
                            }
                        </li>
                    }
                </ul>
                @*点击切换对应菜单*@
                <div id="myTabContent" class="tab-content">
                    @*粤菜*@
                    <div class="tab-pane fade" id="CantoneseCuisine">
                        <table class="table table-bordered">
                            <tr>
                                <th>编号</th>
                                <th>美食名称</th>
                                <th>图片</th>
                                <th>类别</th>
                                <th>难度系数</th>
                                <th>操作</th>
                            </tr>

                            @foreach (var item in CantoneseCuisine)
                            {
                                <tr>
                                    <td>@ranknum</td>
                                    <td>@item.FoodName</td>
                                    <td><img width="130" height="80" src="~/images/food/@item.FoodPicture" /></td>
                                    <td>@item.FoodType</td>
                                    <td>@item.FoodDifficulty</td>
                                    <td>
                                        <a class="btn btn-default" href="#">详情</a>
                                        <a class="btn btn-danger" href="#">❤</a>
                                    </td>
                                </tr>
                                ranknum++;
                            }
                        </table>
                        <div>
                            每页 @Model.PageSize 记录，总有 @Model.PageCount 页，当前 @Model.PageNumber 页
